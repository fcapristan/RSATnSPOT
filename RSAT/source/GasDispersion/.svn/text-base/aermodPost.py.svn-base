import numpy as np

def reader(fileName):
    inputFile = open(fileName,'r')
    index = 0
    x = []
    y = []
    conc = []
    for line in inputFile:
        key = line.split()
        index = index + 1
        if index>8 and len(key)>8:
            x.append(float(key[3]))
            y.append(float(key[4]))
            conc.append(float(key[8]))
    x = np.array(x)
    y = np.array(y)
    conc = np.array(conc)
    return (x,y,conc)
            
